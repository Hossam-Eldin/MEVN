 is called!\n\n    if (!auth) {\n      next();\n    } else {\n      next('/welcome');\n    }\n  },\n  methods: {\n    onAddHobby: function onAddHobby() {\n      var newHobby = {\n        id: Math.random() * Math.random() * 1000,\n        value: ''\n      };\n      this.hobbyInputs.push(newHobby);\n    },\n    onDeleteHobby: function onDeleteHobby(id) {\n      this.hobbyInputs = this.hobbyInputs.filter(function (hobby) {\n        return hobby.id !== id;\n      });\n    },\n    submit: function submit() {\n      var formData = {\n        email: this.email,\n        password: this.password,\n        name: this.name\n      };\n      console.log(formData);\n      /*       axios.post('http://localhost:3000/users/signup',formData)\n            .then(respons => {\n              console.log(respons);\n            })\n            .catch( err => {\n              console.log(err);\n            }) */\n\n      this.$store.dispatch('signup', formData);\n    }\n    /*       onSubmit () {\n            const formData = {\n              email: this.email,\n              age: this.age,\n              password: this.password,\n              confirmPassword: this.confirmPassword,\n              country: this.country,\n              hobbies: this.hobbyInputs.map(hobby => hobby.value),\n              terms: this.terms\n            }\n            console.log(formData)\n            this.$store.dispatch('signup', formData)\n          } */\n\n  }\n};",{"version":3,"sources":["signup.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAgFA,OAAA,KAAA,MAAA,OAAA;AAEA,eAAA;AACA,EAAA,IADA,kBACA;AACA,WAAA;AACA,MAAA,KAAA,EAAA,EADA;AAEA,MAAA,IAAA,EAAA,EAFA;AAGA,MAAA,GAAA,EAAA,IAHA;AAIA,MAAA,QAAA,EAAA,EAJA;AAKA,MAAA,eAAA,EAAA,EALA;AAMA,MAAA,OAAA,EAAA,KANA;AAOA,MAAA,WAAA,EAAA,EAPA;AAQA,MAAA,KAAA,EAAA;AARA,KAAA;AAUA,GAZA;AAaA,EAAA,gBAbA,4BAaA,EAbA,EAaA,IAbA,EAaA,IAbA,EAaA;AACA,QAAA,IAAA,GAAA,YAAA,CAAA,OAAA,CAAA,OAAA,CAAA,CADA,CAEA;AACA;AACA;;AACA,Q